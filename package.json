{
	"name": "userscripts",
	"version": "0.0.0",
	"description": "UserScript environment for OpenUserJS",
	"scripts": {
		"build": "del dist; tsc --declaration",
		"watch": "run-p watch:*",
		"watch:serve": "http-server -c-1 -a 127.0.0.1 -p 8080 dist",
		"watch:tsc": "del dist; tsc --watch --preserveWatchOutput"
	},
	"dependencies": {
		"lodash": "^4.17.20",
		"rambdax": "^7.1.0",
		"string-fn": "^3.1.0",
		"tslib": "^2.1.0"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.167",
		"@types/node": "^14.14.20",
		"del-cli": "^3.0.1",
		"dts-generate": "^1.5.0",
		"http-server": "^0.12.3",
		"node-env-dev": "^1.9.1",
		"npm-run-all": "^4.1.5",
		"typescript": "^4.1.3"
	},
